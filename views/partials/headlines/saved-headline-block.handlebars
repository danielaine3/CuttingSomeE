{{#each headline}}
	<div class="card">
		<div class="card-body">
			<img src="{{this.pic}}">
			<br>
			<p class="title">{{this.title}}</p>
			<br>
			<a class="link" href="www.eonline.com" + {{this.link}}>www.eonline.com{{this.link}}</a>
			<br>
			<a><button class="remove-save" data-id="{{this._id}}">Delete Headline</button></a>
			<a><button class="remove-save" data-id="{{this._id}}">Comment</button></a>
			<br><br>
		</div>
	</div>
{{/each}}