<div class="sticky-top" id="navbar">
	<ul>
		<li><button class="main-button" id="save">SAVE<img class="elogo" src="./assets/images/E!.png">D</button></li>
		<li><button class="main-button" id="scrape">CUT NE<img class="elogo" src="./assets/images/E!.png">W</button></li>
	</ul>
</div>

{{#each headline}}
	<div class="card row mx-auto">
		<div class="display col-sm-12 mx-auto">
			<h2 class="title">{{this.title}}</h2>
			<img src="{{this.pic}}">
			<br>
			<a class= "link" href="http://www.eonline.com{{this.link}}"><button>READ MORE</button></a>
			<a><button class="save-it" data-id="{{this._id}}">SAVE HEADLINE</button></a>
			<br><br>
		</div>
	</div>
{{/each}}